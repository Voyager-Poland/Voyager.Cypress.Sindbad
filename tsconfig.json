    "lib": ["ES6", "DOM"],
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true, 
    "types": ["cypress"] 
  },
  "include": ["cypress/**/*.ts"]
}
